$(function() { 
		$( "#idstart" ).datepicker({
			showOtherMonths: true,
			selectOtherMonths: true,
			numberOfMonths: 1,
			closeText: 'Fermer',
			showButtonPanel: true,
			showOn: "both", //"button",
			buttonImage: "../images/calendar.gif",
			buttonImageOnly: true,
			firstDay: 1,
							minDate: new Date()
		}); 
		$( "#idstart" ).css('position', "relative");
		$( "#idstart" ).datepicker( "option", "dateFormat", 'dd/mm/yy' );
		$( "#idstart" ).datepicker( "setDate" , '09/09/2014');
		$( "#idstart" ).change(function() {
		var date1 = $('#idstart').datepicker('getDate');
		var date = new Date( Date.parse( date1 ) );
		date.setDate( date.getDate() + 1 );
		var newDate = date.toDateString();
		newDate = new Date( Date.parse( newDate ) );
		$('#idend').datepicker('setDate', newDate );
			});

 function SoloDomingos(date){
          var day = date.getDay();
                      return [(day < 1), ''];
      }; 
});
</script>
